img.fullwidth-img {
  width: 100%; }

.hidden {
  display: none; }

.mobile-break {
  display: none; }

main {
  padding-top: 60px; }

.hide-desktop {
  display: none; }

iframe {
  padding-bottom: 15px; }

@media screen and (max-width: 1024px) {
  .display-laptop {
    display: block; }
  .hide-laptop {
    display: none; } }

@media screen and (max-width: 900px) {
