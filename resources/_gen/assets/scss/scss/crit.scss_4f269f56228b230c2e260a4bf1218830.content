.button {
  padding: 12px 30px;
  font-size: 14px;
  justify-content: center;
  align-items: center;
  white-space: nowrap;
  text-decoration: none;
  cursor: pointer;
  outline: none;
  overflow: hidden;
  position: relative;
  display: inline-flex;
  color: #fff;
  background: 0 0;
  z-index: 1; }

.button.fill {
  -webkit-box-shadow: none;
  box-shadow: none;
  -webkit-transition: all .2s ease;
  -o-transition: all .2s ease;
  transition: all .2s ease;
  line-height: 1.4;
  font-weight: 600;
  font-family: lato,sans-serif;
  -webkit-tap-highlight-color: transparent;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  border: none;
  border-radius: 0; }

.button.fill::before {
  content: '';
  position: absolute;
  top: 0;
  right: -50px;
  bottom: 0;
  left: 0;
  position: absolute;
  content: '';
  z-index: -1;
  top: 0;
  left: -10%;
  right: 0;
  bottom: 0;
  width: 0;
  background: #1badff;
  -webkit-transform: skew(-15deg);
  -ms-transform: skew(-15deg);
  transform: skew(-15deg);
  -webkit-transform-origin: 0 50%;
  -ms-transform-origin: 0 50%;
  transform-origin: 0 50%;
  -webkit-transition: all .5s;
  -o-transition: all .5s;
  transition: all .5s; }

.button.fill:hover {
  -webkit-box-shadow: none;
  box-shadow: none; }

.button.fill:hover::before {
  width: 60%; }

.button.fill:hover, .button.icon:hover {
  color: #fff;
  transition: all .2s ease; }

