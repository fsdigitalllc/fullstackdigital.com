.featured-logos{display:flex;justify-content:space-between;align-items:center;margin-left:auto;margin-right:auto}.featured-logos .ddn img{height:40px}.featured-logos .dell img{height:20px}.featured-logos .rigado img{height:26px}.featured-logos .quantum img{height:22px}.featured-logos .threefactor img{height:40px}.featured-logos .code img{height:40px}@media screen and (max-width:900px){.featured-logos{display:flex;flex-wrap:wrap;justify-content:center;max-width:630px}.featured-logos img{margin:10px 20px}}@media screen and (max-width:480px){.featured-logos{display:grid;grid-template-columns:repeat(2,1fr);grid-template-rows:repeat(3,1fr);grid-column-gap:30px;grid-row-gap:20px;justify-content:center;align-items:center}.featured-logos img{margin:auto;display:block}}.template-service_process .col-3{flex-basis:28%}.template-service_process .perspective-items{position:relative;padding:50px;perspective:500px}.template-service_process .perspective-item{padding:60px 0;margin:-120px 0;transform-origin:center center}.template-service_process .perspective-item:nth-child(1){background:#96e4ff;transform:rotateX(50deg);transition:transform .5s linear}.template-service_process .perspective-item:nth-child(1):hover{transform:rotateX(45deg) translateZ(10px);transition:transform .5s linear}.template-service_process .perspective-item:nth-child(2){background:#5ec5ff;transform:rotateX(65deg);transition:transform .5s linear}.template-service_process .perspective-item:nth-child(2):hover{transform:rotateX(60deg) translateZ(10px);transition:transform .5s linear}.template-service_process .perspective-item:nth-child(3){background:#0069f1;transform:rotateX(80deg);transition:transform .5s linear}.template-service_process .perspective-item:nth-child(3):hover{transform:rotateX(75deg) translateZ(10px);transition:transform .5s linear}.template-service_process .perspective-item svg{display:block;margin:auto}.template-service_process .black-number{background:#000;border-radius:50%;display:flex;width:40px;height:40px;text-align:center;margin:0 auto;justify-content:center;margin-bottom:15px;align-items:center}.template-service_process #number-4-row{display:grid;grid-template-columns:.8fr 1.5fr 2.4fr 1fr}.template-service_process .border-dark{height:1px;background-color:#666}.template-service_process .number-title{font-size:20px;font-weight:300;line-height:1.25}.template-service_process .black-number>span{font-size:20px;line-height:1.5em;font-weight:900}